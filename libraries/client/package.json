{
  "name": "@repo/client",
  "version": "0.0.3",
  "description": "Client for betro api",
  "main": "dist/index.js",
  "private": false,
  "engines": {
    "node": ">=16.0.0"
  },
  "scripts": {
    "build": "npm run build-ts && npm run lint",
    "build-ts": "tsc",
    "lint": "eslint \"**/*.{js,ts}\" --quiet --fix",
    "prettier": "prettier src test --write",
    "test": "jest --coverage",
    "example:generate": "ts-node test/generateUsers.ts",
    "watch-test": "npm run test -- --watchAll",
    "watch-ts": "tsc -w"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/prijindal/betro-js-client.git"
  },
  "keywords": [
    "betro"
  ],
  "author": "Priyanshu Jindal",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/prijindal/betro-js-client/issues"
  },
  "homepage": "https://github.com/prijindal/betro-js-client#readme",
  "dependencies": {
    "@repo/encryption": "*",
    "axios": "^1.3.6",
    "buffer": "^6.0.3",
    "casual": "^1.6.2",
    "lodash": "^4.17.21"
  },
  "devDependencies": {
    "@types/jest": "^29.5.11",
    "@types/lodash": "^4.14.202",
    "@types/node": "^18.19.3",
    "@typescript-eslint/eslint-plugin": "^5.59.1",
    "@typescript-eslint/parser": "^5.59.1",
    "eslint": "^8.39.0",
    "eslint-config-prettier": "^8.8.0",
    "husky": "^8.0.3",
    "jest": "^29.5.0",
    "jest-puppeteer": "^8.0.6",
    "nodemon": "^2.0.22",
    "pre-commit": "^1.2.2",
    "prettier": "^2.8.8",
    "puppeteer": "^19.11.1",
    "ts-jest": "^29.1.0",
    "ts-node": "^10.9.1",
    "typescript": "^5.0.4"
  }

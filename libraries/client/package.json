{
  "name": "@prijindal/betro-js-client",
  "version": "0.0.3",
  "description": "Client for betro api",
  "main": "dist/index.js",
  "private": false,
  "engines": {
    "node": ">=16.0.0"
  },
  "scripts": {
    "build": "npm run build-ts && npm run lint",
    "build-ts": "tsc",
    "lint": "eslint \"**/*.{js,ts}\" --quiet --fix",
    "prettier": "prettier src test --write",
    "test": "jest --coverage",
    "example:generate": "ts-node test/generateExample.ts",
    "watch-test": "npm run test -- --watchAll",
    "watch-ts": "tsc -w"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/prijindal/betro-js-client.git"
  },
  "keywords": [
    "betro"
  ],
  "author": "Priyanshu Jindal",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/prijindal/betro-js-client/issues"
  },
  "homepage": "https://github.com/prijindal/betro-js-client#readme",
  "dependencies": {
    "@prijindal/betro-js-lib": "file:../encryption",
    "axios": "^0.27.2",
    "buffer": "^6.0.3",
    "casual": "^1.6.2",
    "lodash": "^4.17.21"
  },
  "devDependencies": {
    "@types/jest": "^28.1.6",
    "@types/lodash": "^4.14.182",
    "@types/node": "^18.6.5",
    "@typescript-eslint/eslint-plugin": "^5.33.0",
    "@typescript-eslint/parser": "^5.33.0",
    "eslint": "^8.21.0",
    "eslint-config-prettier": "^8.5.0",
    "husky": "^8.0.1",
    "jest": "^28.1.3",
    "jest-puppeteer": "^6.1.1",
    "nodemon": "^2.0.19",
    "pre-commit": "^1.2.2",
    "prettier": "^2.7.1",
    "puppeteer": "^16.1.0",
    "ts-jest": "^28.0.7",
    "ts-node": "^10.9.1",
    "typescript": "^4.7.4"
  }
}

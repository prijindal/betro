{
  "name": "betro-web",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@prijindal/betro-js-client": "file:../../libraries/client",
    "@prijindal/betro-js-lib": "file:../../libraries/encryption",
    "@heroicons/react": "^1.0.6",
    "@mantine/core": "^5.1.1",
    "@mantine/hooks": "^5.1.1",
    "@tailwindcss/forms": "^0.5.2",
    "@tailwindcss/typography": "^0.5.4",
    "axios": "^0.27.2",
    "eslint-config-react-app": "^7.0.1",
    "immer": "^9.0.15",
    "lodash": "^4.17.21",
    "marked": "^4.0.18",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-helmet-async": "^1.3.0",
    "react-infinite-scroll-hook": "^4.0.3",
    "react-redux": "^8.0.2",
    "react-router": "^6.3.0",
    "react-router-dom": "^6.3.0",
    "redux": "^4.2.0",
    "redux-thunk": "^2.4.1",
    "reselect": "^4.1.6",
    "sanitize-html": "^2.7.1",
    "sass": "^1.54.4",
    "source-map-explorer": "^2.5.2",
    "web-vitals": "^2.1.4"
  },
  "scripts": {
    "analyze": "source-map-explorer 'build/static/js/*.js'",
    "start": "vite",
    "build": "tsc && vite build",
    "build:docker": "docker build -t betro/web .",
    "docker:publish": "docker push betro/web:latest",
    "build-with-stats": "npm run build && npm run analyze",
    "depgraph": "depcruise -c .dependency-cruiser.js --exclude \"node_modules/*\" --output-type dot src | dot -T svg > dependency-graph.svg",
    "lint": "concurrently \"yarn-or-npm run lint:js\" \"yarn-or-npm run lint:css\"",
    "lint:js": "eslint --ext .js,.jsx,.ts,.tsx src/",
    "lint:css": "stylelint src/**/*.css src/**/*.scss",
    "lint:deps": "depcruise -c .dependency-cruiser.js src/",
    "eject": "react-scripts eject",
    "tsc": "tsc --noEmit"
  },
  "devDependencies": {
    "@babel/core": "^7.18.10",
    "@esbuild-plugins/node-globals-polyfill": "^0.1.1",
    "@testing-library/dom": "^8.17.1",
    "@testing-library/jest-dom": "^5.16.5",
    "@testing-library/react": "^13.3.0",
    "@testing-library/user-event": "^14.4.3",
    "@types/jest": "^28.1.6",
    "@types/lodash": "^4.14.182",
    "@types/marked": "^4.0.3",
    "@types/node": "^18.6.5",
    "@types/react": "^18.0.17",
    "@types/react-dom": "^18.0.6",
    "@types/react-router": "^5.1.18",
    "@types/react-router-dom": "^5.3.3",
    "@types/sanitize-html": "^2.6.2",
    "@typescript-eslint/eslint-plugin": "^5.33.0",
    "@vitejs/plugin-react": "^2.0.0",
    "autoprefixer": "^10.4.8",
    "browserslist": "^4.21.3",
    "compression-webpack-plugin": "^10.0.0",
    "concurrently": "^7.3.0",
    "depcheck": "^1.4.3",
    "dependency-cruiser": "^11.14.1",
    "eslint": "^8.21.0",
    "husky": "^8.0.1",
    "jest-puppeteer": "^6.1.1",
    "postcss": "^8.4.16",
    "prettier": "^2.7.1",
    "puppeteer": "^16.1.0",
    "rollup-plugin-node-polyfills": "^0.2.1",
    "scss": "^0.2.4",
    "stylelint": "^14.9.1",
    "stylelint-config-prettier": "^9.0.3",
    "stylelint-order": "^5.0.0",
    "stylelint-prettier": "^2.0.0",
    "tailwindcss": "3.1.8",
    "typescript": "4.7.4",
    "vite": "^3.0.5",
    "yarn-or-npm": "^3.0.1"
  },
  "resolutions": {
    "caniuse-lite": "^1.0.30001228"
  },
  "eslintConfig": {
    "overrides": [
      {
        "files": [
          "**/*.stories.*"
        ],
        "rules": {
          "import/no-anonymous-default-export": "off"
        }
      }
    ]
  }
}
